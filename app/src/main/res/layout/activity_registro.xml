<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#FFFFFFFF"
    tools:context=".RegistroActivity">

  <TextView
      android:id="@+id/textView"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:layout_marginTop="80dp"
      android:text="@string/registro"
      android:textColor="#5271FF"
      android:textSize="60sp"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toTopOf="parent" />

  <Button
      android:id="@+id/button"
      android:layout_width="69dp"
      android:layout_height="33dp"
      android:background="@color/black"
      android:backgroundTint="@color/base_blue"
      android:textColor="@color/base_blue"
      android:textColorHighlight="@color/base_blue"
      android:textColorHint="@color/base_blue"
      android:textColorLink="#5271FF"
      app:icon="@android:drawable/ic_menu_revert"
      app:iconTint="#FFFFFF"
      app:rippleColor="@color/base_blue"
      app:strokeColor="@color/base_blue"
      tools:ignore="MissingConstraints,SpeakableTextPresentCheck"
      tools:layout_editor_absoluteX="0dp"
      tools:layout_editor_absoluteY="-1dp" />

  <EditText
      android:id="@+id/editTextText"
      android:layout_width="156dp"
      android:layout_height="48dp"
      android:layout_marginTop="48dp"
      android:autofillHints=""
      android:ems="10"
      android:inputType="text"
      android:text="Name"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintHorizontal_bias="0.079"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toBottomOf="@+id/textView"
      tools:ignore="DuplicateSpeakableTextCheck,LabelFor" />

  <EditText
      android:id="@+id/editTextText2"
      android:layout_width="200dp"
      android:layout_height="48dp"
      android:layout_marginTop="48dp"
      android:layout_marginEnd="16dp"
      android:ems="10"
      android:inputType="text"
      android:text="Name"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintTop_toBottomOf="@+id/textView" />

  <EditText
      android:id="@+id/editTextNumber"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:ems="10"
      android:inputType="number"
      tools:layout_editor_absoluteX="175dp"
      tools:layout_editor_absoluteY="256dp" />

  <EditText
      android:id="@+id/editTextText3"
      android:layout_width="136dp"
      android:layout_height="45dp"
      android:ems="10"
      android:inputType="text"
      android:text="Name"
      app:layout_constraintEnd_toStartOf="@+id/editTextNumber"
      app:layout_constraintStart_toStartOf="parent"
      tools:layout_editor_absoluteY="265dp" />

  <EditText
      android:id="@+id/editTextNumber2"
      android:layout_width="365dp"
      android:layout_height="41dp"
      android:ems="10"
      android:inputType="number"
      tools:layout_editor_absoluteX="24dp"
      tools:layout_editor_absoluteY="325dp" />

  <EditText
      android:id="@+id/editTextTextEmailAddress"
      android:layout_width="356dp"
      android:layout_height="44dp"
      android:ems="10"
      android:inputType="textEmailAddress"
      tools:layout_editor_absoluteX="27dp"
      tools:layout_editor_absoluteY="384dp" />

  <EditText
      android:id="@+id/editTextTextPassword"
      android:layout_width="353dp"
      android:layout_height="50dp"
      android:ems="10"
      android:inputType="textPassword"
      tools:layout_editor_absoluteX="28dp"
      tools:layout_editor_absoluteY="481dp" />

  <EditText
      android:id="@+id/editTextTextPassword2"
      android:layout_width="348dp"
      android:layout_height="46dp"
      android:ems="10"
      android:inputType="textPassword"
      tools:layout_editor_absoluteX="28dp"
      tools:layout_editor_absoluteY="569dp" />

  <Button
      android:id="@+id/button2"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:backgroundTint="@color/base_blue"
      android:text="Siguiente"
      tools:layout_editor_absoluteX="151dp"
      tools:layout_editor_absoluteY="633dp" />

  <TextView
      android:id="@+id/textView5"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:text="APPARKING"
      android:textColor="#5271FF"
      tools:layout_editor_absoluteX="167dp"
      tools:layout_editor_absoluteY="696dp" />

</androidx.constraintlayout.widget.ConstraintLayout>